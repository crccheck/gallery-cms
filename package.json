{
  "name": "gallery-cms",
  "version": "1.1.0",
  "scripts": {
    "build:css": "node-sass src/styles/ --include-path node_modules --output app",
    "build:js": "browserify src/scripts/main.js -t babelify -o app/app.js",
    "build": "npm run build:js && npm run build:css",
    "watch:css": "node-sass src/styles/ --include-path node_modules --output app --watch",
    "watch:js": "watchify src/scripts/main.js -t babelify -o app/app.js",
    "serve": "nodemon --ext py -x python gallery/gallery.py ~/Sync/Photos/buttons --admin ${ADMIN_EMAIL} --no-save-originals",
    "proxy": "browser-sync start --proxy \"localhost:8080\" --files app --files templates --no-open",
    "dev": "concurrently --kill-others 'npm run serve' 'npm run watch:js' 'npm run watch:css' 'npm run proxy'"
  },
  "description": "",
  "main": "",
  "private": true,
  "author": "Chris Chang <c@crccheck.com> (https://crccheck.com/blog)",
  "engines": {
    "node": ">=6.11"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "concurrently": "^3.5.0",
    "jquery": "^3.2.1",
    "js-crc": "^0.2.0",
    "lazysizes": "^3.0.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "normalize.sass": "^3.0.3-2",
    "select2": "^4.0.3",
    "standard": "^10.0.3",
    "watchify": "^3.9.0"
  },
  "dependencies": {}
}
